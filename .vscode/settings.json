{
  "workbench.colorTheme": "August - Kanagawa (Darker)",
  "workbench.iconTheme": "theme",
  "editor.fontFamily": "Space Mono",
  "scm.inputFontFamily": "Space Mono",
  "terminal.integrated.fontFamily": "monospace",
  "chat.editor.fontFamily": "Space Mono",
  "debug.console.fontFamily": "Space Mono",
  "editor.codeLensFontFamily": "Space Mono",
  "notebook.output.fontFamily": "Space Mono",
  "markdown.preview.fontFamily": "Space Mono",
  "editor.inlayHints.fontFamily": "Space Mono",
